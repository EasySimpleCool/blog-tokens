{
  "modify": {
    "emphasis": {
      "interaction": {
        "surface": {
          "fill": {
            "$type": "color",
            "$value": "{modify.interaction.color.accent.color-opacity}"
          }
        },
        "content": {
          "fill": {
            "$type": "color",
            "$value": "{modify.displaymode.color.fg.primary}"
          }
        },
        "stroke": {
          "color": {
            "$type": "color",
            "$value": "null"
          }
        }
      }
    }
  }
}