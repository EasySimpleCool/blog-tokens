{
  "input": {
    "input": {
      "dimension": {
        "0": {
          "$type": "dimension",
          "$value": "0px"
        },
        "4": {
          "$type": "dimension",
          "$value": "4px"
        },
        "8": {
          "$type": "dimension",
          "$value": "8px"
        },
        "12": {
          "$type": "dimension",
          "$value": "12px"
        },
        "16": {
          "$type": "dimension",
          "$value": "16px"
        },
        "18": {
          "$type": "dimension",
          "$value": "18px"
        },
        "24": {
          "$type": "dimension",
          "$value": "24px"
        },
        "40": {
          "$type": "dimension",
          "$value": "40px"
        },
        "80": {
          "$type": "dimension",
          "$value": "80px"
        }
      },
      "color": {
        "blue": {
          "250": {
            "$type": "color",
            "$value": "#5DA6D7"
          },
          "500": {
            "$type": "color",
            "$value": "#0A91CC"
          }
        },
        "light": {
          "250": {
            "$type": "color",
            "$value": "#F7F9FB"
          },
          "500": {
            "$type": "color",
            "$value": "#E8EDF4"
          },
          "750": {
            "$type": "color",
            "$value": "#D9E1EC"
          }
        },
        "dark": {
          "250": {
            "$type": "color",
            "$value": "#545F6D"
          },
          "500": {
            "$type": "color",
            "$value": "#343A41"
          }
        }
      }
    }
  },
  "modify": {
    "modify": {
      "dimension": {
        "small": {
          "$type": "dimension",
          "$value": "{input.dimension.18}"
        },
        "medium": {
          "$type": "dimension",
          "$value": "{input.dimension.24}"
        },
        "large": {
          "$type": "dimension",
          "$value": "{input.dimension.40}"
        },
        "xlarge": {
          "$type": "dimension",
          "$value": "{input.dimension.80}"
        }
      }
    }
  },
  "output": {
    "output": {
      "typography": {
        "tiny": {
          "$type": "typography",
          "$value": {
            "fontFamily": "Inter",
            "fontSize": "{input.dimension.12}",
            "fontWeight": "Regular"
          }
        },
        "small": {
          "$type": "typography",
          "$value": {
            "fontFamily": "Inter",
            "fontSize": "{input.dimension.18}",
            "fontWeight": "Regular"
          }
        },
        "medium": {
          "$type": "typography",
          "$value": {
            "fontFamily": "Inter",
            "fontSize": "{input.dimension.24}",
            "fontWeight": "Semi Bold"
          }
        },
        "large": {
          "$type": "typography",
          "$value": {
            "fontFamily": "Inter",
            "fontSize": "{input.dimension.40}",
            "fontWeight": "Black"
          }
        }
      },
      "color": {
        "accent": {
          "primary": {
            "$type": "color",
            "$value": "{input.color.blue.500}"
          },
          "lighter": {
            "$type": "color",
            "$value": "{input.color.blue.250}"
          },
          "on-accent": {
            "$type": "color",
            "$value": "{input.color.light.250}"
          }
        },
        "fg": {
          "primary": {
            "$type": "color",
            "$value": "{input.color.dark.500}"
          },
          "secondary": {
            "$type": "color",
            "$value": "{input.color.dark.250}"
          }
        },
        "bg": {
          "above": {
            "$type": "color",
            "$value": "{input.color.light.250}"
          },
          "middle": {
            "$type": "color",
            "$value": "{input.color.light.500}"
          },
          "below": {
            "$type": "color",
            "$value": "{input.color.light.750}"
          }
        },
        "border": {
          "default": {
            "$type": "color",
            "$value": "{input.color.light.250}"
          }
        }
      },
      "dimension": {
        "fixed-0": {
          "$type": "dimension",
          "$value": "{input.dimension.0}"
        },
        "fixed-4": {
          "$type": "dimension",
          "$value": "{input.dimension.4}"
        },
        "fixed-8": {
          "$type": "dimension",
          "$value": "{input.dimension.8}"
        },
        "fixed-16": {
          "$type": "dimension",
          "$value": "{input.dimension.16}"
        },
        "fixed-24": {
          "$type": "dimension",
          "$value": "{input.dimension.24}"
        },
        "fixed-40": {
          "$type": "dimension",
          "$value": "{input.dimension.40}"
        },
        "fixed-80": {
          "$type": "dimension",
          "$value": "{input.dimension.80}"
        }
      },
      "border-radius": {
        "small": {
          "$type": "borderRadius",
          "$value": "{input.dimension.4}"
        },
        "default": {
          "$type": "borderRadius",
          "$value": "{input.dimension.8}"
        }
      }
    }
  },
  "comp": {
    "comp": {
      "section": {
        "h-pad": {
          "default": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-40}"
          }
        },
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{output.color.bg.middle}"
          }
        },
        "v-pad": {
          "default": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-80}",
            "$description": "Use: Body sections "
          },
          "less": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-24}",
            "$description": "Use: Header, Footer"
          }
        }
      },
      "container": {
        "max-w": {
          "default": {
            "$type": "dimension",
            "$value": "640px"
          }
        }
      },
      "box-stack": {
        "pad-gap": {
          "least": {
            "$type": "dimension",
            "$value": "{input.dimension.4}"
          },
          "even-less": {
            "$type": "dimension",
            "$value": "{input.dimension.8}"
          },
          "less": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-16}"
          },
          "default": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-24}"
          },
          "more": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-40}"
          },
          "even-more": {
            "$type": "dimension",
            "$value": "{output.dimension.fixed-80}"
          }
        }
      },
      "post-preview": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{output.color.bg.above}"
          }
        },
        "border": {
          "default": {
            "$type": "color",
            "$value": "{input.color.dark.250}"
          }
        },
        "b-rad": {
          "$type": "dimension",
          "$value": "{output.border-radius.default}"
        },
        "pad": {
          "$type": "dimension",
          "$value": "{output.dimension.fixed-24}"
        }
      },
      "image": {
        "b-rad": {
          "$type": "dimension",
          "$value": "{output.border-radius.small}"
        }
      },
      "button": {
        "primary": {
          "bg": {
            "$type": "color",
            "$value": "{output.color.accent.primary}"
          },
          "fg": {
            "$type": "color",
            "$value": "{output.color.accent.on-accent}"
          }
        },
        "secondary": {
          "border": {
            "$type": "color",
            "$value": "{output.color.accent.lighter}"
          },
          "fg": {
            "$type": "color",
            "$value": "{output.color.accent.primary}"
          }
        }
      },
      "tag": {
        "b-rad": {
          "$type": "dimension",
          "$value": "{output.border-radius.small}"
        },
        "border": {
          "default": {
            "$type": "color",
            "$value": "{input.color.dark.250}"
          }
        },
        "h-pad": {
          "$type": "dimension",
          "$value": "{output.dimension.fixed-8}"
        },
        "v-pad": {
          "$type": "dimension",
          "$value": "{output.dimension.fixed-4}"
        }
      },
      "post-detail": {
        "b-rad": {
          "$type": "dimension",
          "$value": "{output.border-radius.default}"
        },
        "pad": {
          "$type": "dimension",
          "$value": "{output.dimension.fixed-40}"
        },
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{output.color.bg.above}"
          }
        },
        "border": {
          "default": {
            "$type": "color",
            "$value": "{input.color.dark.250}"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "input",
      "modify",
      "output",
      "comp"
    ]
  }
}