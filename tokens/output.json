{
  "output": {
    "typography": {
      "tiny": {
        "$type": "typography",
        "$value": {
          "fontFamily": "Inter",
          "fontSize": "{input.dimension.12}",
          "fontWeight": "Regular"
        }
      },
      "small": {
        "$type": "typography",
        "$value": {
          "fontFamily": "Inter",
          "fontSize": "{input.dimension.18}",
          "fontWeight": "Regular"
        }
      },
      "medium": {
        "$type": "typography",
        "$value": {
          "fontFamily": "Inter",
          "fontSize": "{input.dimension.24}",
          "fontWeight": "Semi Bold"
        }
      },
      "large": {
        "$type": "typography",
        "$value": {
          "fontFamily": "Inter",
          "fontSize": "{input.dimension.40}",
          "fontWeight": "Black"
        }
      }
    },
    "color": {
      "accent": {
        "primary": {
          "$type": "color",
          "$value": "{input.color.blue.500}"
        },
        "lighter": {
          "$type": "color",
          "$value": "{input.color.blue.250}"
        },
        "on-accent": {
          "$type": "color",
          "$value": "{input.color.light.250}"
        }
      },
      "fg": {
        "primary": {
          "$type": "color",
          "$value": "{input.color.dark.500}"
        },
        "secondary": {
          "$type": "color",
          "$value": "{input.color.dark.250}"
        }
      },
      "bg": {
        "above": {
          "$type": "color",
          "$value": "{input.color.light.250}"
        },
        "middle": {
          "$type": "color",
          "$value": "{input.color.light.500}"
        },
        "below": {
          "$type": "color",
          "$value": "{input.color.light.750}"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{input.color.light.250}"
        }
      }
    },
    "dimension": {
      "fixed-0": {
        "$type": "dimension",
        "$value": "{input.dimension.0}"
      },
      "fixed-4": {
        "$type": "dimension",
        "$value": "{input.dimension.4}"
      },
      "fixed-8": {
        "$type": "dimension",
        "$value": "{input.dimension.8}"
      },
      "fixed-16": {
        "$type": "dimension",
        "$value": "{input.dimension.16}"
      },
      "fixed-24": {
        "$type": "dimension",
        "$value": "{input.dimension.24}"
      },
      "fixed-40": {
        "$type": "dimension",
        "$value": "{input.dimension.40}"
      },
      "fixed-80": {
        "$type": "dimension",
        "$value": "{input.dimension.80}"
      }
    },
    "border-radius": {
      "small": {
        "$type": "borderRadius",
        "$value": "{input.dimension.4}"
      },
      "default": {
        "$type": "borderRadius",
        "$value": "{input.dimension.8}"
      }
    }
  }
}