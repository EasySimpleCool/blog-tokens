{
  "comp": {
    "button": {
      "primary": {
        "bg-default": {
          "$type": "color",
          "$value": "{modify.accent.color.primary}"
        },
        "bg-hover": {
          "$type": "color",
          "$value": "{modify.accent.color.lighter}"
        },
        "fg": {
          "$type": "color",
          "$value": "{modify.accent.color.on-accent}"
        }
      },
      "min-h": {
        "$type": "dimension",
        "$value": "64px"
      },
      "b-rad": {
        "$type": "dimension",
        "$value": "{output.border-radius.small}"
      },
      "pad": {
        "$type": "dimension",
        "$value": "{output.dimension.fixed-16}"
      }
    }
  }
}